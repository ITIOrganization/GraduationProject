@model IEnumerable<AlumniSystem.Models.JobTitle>

@{
    ViewBag.Title = "ViewAll";
}


<table class="table table-striped table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            <a class="btn btn-primary btn-sm" onclick="Edit('@Url.Action("Addoredit","JobTitle",new { id=item.Id})')"><i class="fa fa-pencil fa-lg"></i></a>
            <a class="btn btn-danger btn-sm" onclick="Delete('@Url.Action("Delete","JobTitle",new { id=item.Id})')"><i class="fa fa-trash fa-lg"></i></a>
        </td>
    </tr>
}

</table>
